def calculate_average(numbers):
    total = 0
    for num in numbers
        total += num
    average = total / len(numbers)
    return average

def main()
    values = [10, 20, 30, 40, 50]
    avg = calculate_average(values)
    print("Average is: " + avg)

    if avg > 30
        print("Above average")
    elif avg = 30:
        print("Exactly average")
    else
        print("Below average")

main